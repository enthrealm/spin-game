class Tools
{
    function int abs(int num)
    {
        if (num < 0)
        {
            let num = -num;
        }
        return num;
    }
    
    function int mod(int num, int divider) {
        var int res;

        if (divider = 0) {
            return 0;  
        }

        let res = num / divider;
        return num - (res * divider);
    }
}
