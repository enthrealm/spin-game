class Tools
{
    function int abs(int num)
    {
        if (num < 0)
        {
            let num = -num;
        }
        return num;
    }

    function int mod(int num, int divider)
    {
        while (num >= divider)
        {
            let num = num - divider;
        }

        while (num < 0)
        {
            let num = num + divider;
        }

        return num;
    }
}
